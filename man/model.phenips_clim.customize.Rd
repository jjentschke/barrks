      # dev_rates = matrix(...),

      model_end_date = '12-31',

      # ==== diapause ====

      first_diapause_date = '08-12',
      diapause_thermal_func = function(daylength, tmax) \{
        0.8619156 * daylength + 0.5081128 * tmax - 23.63691 > 0
      \},
      daylength_dia = 14.5,

      # ==== mortality ====

      tlethal = -5
