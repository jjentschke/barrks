      onset_func = \\(tmax, dd_tmax) \{
        0.564071 * tmax + 0.006434 * dd_tmax - 12.37046 > 0
      \},

      dd_onset_alt_start_date = '04-01',
      dd_onset_alt_base = 8.3,
      onset_alt_func = \\(tmax, dd_tmax) dd_tmax >= 140,

